<!-- Main content -->
<section class="content">
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">Lịch Sử Danh Mục ĐKSX/Program Pdt Control History</h3>
            <div class="box-tools pull-right">
            </div>
        </div>
        <div class="box-body">
            <div class="row">
                <div class="col-xs-2">
                    <div class="form-group">
                        <label for="SearchProgramName">ProgramName</label>
                        <input type="text" class="form-control" id="SearchProgramName" placeholder="SearchProgramName">
                    </div>
                </div>
                <div class="col-xs-2">
                    <div class="form-group">
                        <label for="SearchPart">Part</label>
                        <input type="text" class="form-control" id="SearchPart" placeholder="SearchPart">
                    </div>
                </div>
                <div class="col-xs-2">
                    <div class="form-group">
                        <label for="SearchControlItem">ControlItem</label>
                        <input type="text" class="form-control" id="SearchControlItem" placeholder="SearchControlItem">
                    </div>
                </div>
                <div class="col-xs-2">
                    <div class="form-group">
                        <label for="SearchMaker">Maker</label>
                        <input type="text" class="form-control" id="SearchMaker" placeholder="SearchMaker">
                    </div>
                </div>
                <div class="col-xs-4">
                    <div class="col-xs-6">
                        <div class="form-group">
                            <label for="btnSearch">&nbsp;</label>
                            <button type="button" class="btn btn-primary form-control pull-left" id="btnSearch" onclick="return ProgramPdtCtrlHistory_SearchTools(1);">&nbsp;&nbsp;Search&nbsp;</button>
                        </div>
                    </div>
                    <div class="col-xs-6">
                        <div class="form-group">
                            <label for="btnSearch">&nbsp;</label>
                            <button type="button" class="btn btn-primary form-control pull-right" id="btnClearSearchItem" onclick="return ClearSearchItem();">Clear Search</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-sm-12 col-xs-12">
                    <table id="PdtCtrlHistory_DataTable" class="table table-striped table-bordered table-hover text-nowrap" style="width:100%">
                        <thead>
                            <tr class="bg-light-blue color-palette text-center">
                                <th>
                                    ProgramName
                                </th>
                                <th>
                                    Part
                                </th>
                                <th>
                                    ControlItem
                                </th>
                                <th>
                                    ColumnName
                                </th>
                                <th>
                                    SpecDisplay
                                </th>
                                <th>
                                    Unit
                                </th>
                                <th>
                                    LowerLimit
                                </th>
                                <th>
                                    UpperLimit
                                </th>
                                <th>
                                    Created Date
                                </th>
                                <th>
                                    Operator
                                </th>
                                <th>
                                    ItemName
                                </th>
                                <th>
                                    History Date
                                </th>
                                <th>
                                    History Operator
                                </th>
                                <th>
                                    Status
                                </th>
                                <th>
                                    Action
                                </th>
                            </tr>
                        </thead>
                        <tbody class="tbody"></tbody>
                    </table>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 text-center" id="page-selection">
                </div>
            </div>
        </div>
    </div>
